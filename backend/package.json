{
  "name": "insurance-voice-ai-backend",
  "version": "1.0.0",
  "description": "Backend API for Insurance Voice AI POC",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "node --watch src/index.js",
    "start": "node src/index.js",
    "db:init": "node scripts/init-db.js",
    "db:seed": "node scripts/seed-data.js",
    "db:reset": "rm -f data/database.sqlite && npm run db:init && npm run db:seed"
  },
  "dependencies": {
    "better-sqlite3": "^9.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "twilio": "^4.23.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
